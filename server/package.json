{
  "name": "eagle-io",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:build": "docker-compose -f ../docker/docker-compose.yml build eagle-io",
    "docker:up": "docker-compose -f ../docker/docker-compose.yml up",
    "docker:up:minio": "docker-compose -f ../docker/docker-compose.yml up minio",
    "docker:down": "docker-compose -f ../docker/docker-compose.yml down",
    "docker:down:minio": "docker-compose -f ../docker/docker-compose.yml down minio",
    "docker:ps": "docker ps",
    "docker:stat": "docker stats",
    "docker:ps2": "docker-compose -f ../docker/docker-compose.yml ps",
    "docker:logs:follow": "docker-compose -f ../docker/docker-compose.yml logs -f",

    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "server:start:dev": "nest start --watch",
    "server:start:debug": "nest start --debug --watch",
    "server:start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.41.0",
    "@nestjs/common": "8.2.1",
    "@nestjs/core": "8.2.1",
    "@nestjs/platform-express": "8.2.1",
    "axios": "0.24.0",
    "dotenv": "10.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.1",
    "@types/express": "4.17.13",
    "@types/node": "16.0.0",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
